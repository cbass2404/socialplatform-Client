n\n  dayjs.extend(relativeTime);\n\n  return (\n    <Card className={classes.card}>\n      <Grid container justify=\"space-between\">\n        <CardMedia\n          image={imageUrl}\n          title=\"Profile image\"\n          className={classes.image}\n        />\n        <Grid item>\n          <CardContent className={classes.content}>\n            <Typography\n              variant=\"h5\"\n              component={Link}\n              to={`/users/${handle}`}\n              color=\"primary\"\n            >\n              {handle}\n            </Typography>\n            <Typography variant=\"body2\" color=\"textSecondary\">\n              {dayjs(createdAt).fromNow()}\n            </Typography>\n            <Typography variant=\"body1\">{body}</Typography>\n            <span>{likeCount} Likes</span>\n            <TipButton tip=\"comments\">\n              <ChatIcon color=\"primary\" />\n            </TipButton>\n            <span>{commentCount} comments</span>\n          </CardContent>\n        </Grid>\n        <Grid item>\n          {authenticated && credentials.handle === handle ? (\n            <Grid container>\n              <Grid item>\n                <Tooltip title=\"Delete\">\n                  <Link onClick={() => deletePost(postId)}>\n                    <DeleteIcon color=\"secondary\" />\n                  </Link>\n                </Tooltip>\n              </Grid>\n            </Grid>\n          ) : null}\n        </Grid>\n      </Grid>\n    </Card>\n  );\n};\n\nPost.propTypes = {\n  deletePost: PropTypes.func.isRequired,\n  user: PropTypes.object.isRequired,\n  post: PropTypes.object.isRequired,\n  classes: PropTypes.object.isRequired,\n};\n\nconst mapStateToProps = (state) => ({\n  user: state.user,\n});\n\nexport default connect(mapStateToProps, { deletePost })(\n  withStyles(styles)(Post)\n);\n","/home/cbass/coding/bottega/class/capstone/final/client/src/components/profile/profile.js",[],"/home/cbass/coding/bottega/class/capstone/final/client/src/comp